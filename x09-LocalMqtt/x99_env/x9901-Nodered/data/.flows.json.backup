    {
        "id": "func_parse_rs232_s2",
        "type": "function",
        "z": "scale_rs232_tab",
        "name": "Parse Weight S2",
        "func": "const raw = msg.payload.toString();\n// Regex to find decimal numbers in strings like 'ST,GS,+  12.34 kg'\nconst match = raw.match(/[-+]?\\d*\\.?\\d+/);\nif (match) {\n    const weight = parseFloat(match[0]);\n    msg.payload = { scale_id: 'scale-02', weight: weight, source: 'rs232' };\n    return msg;\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 400,
        "y": 100,
        "wires": [
            [
                "debug_s2_rs232",
                "disp_s2"
            ]
        ]
    },
