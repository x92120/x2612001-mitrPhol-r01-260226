    {
        "id": "func_s1",
        "type": "function",
        "z": "scale_dashboard_tab",
        "name": "Logic S1",
        "func": "let weight = flow.get('w1') || 0;\nif (msg.payload === 'up') weight += Math.random() * (0.005 - 0.0001) + 0.0001;\nelse if (msg.payload === 'down') weight -= Math.random() * (0.005 - 0.0001) + 0.0001;\nelse if (msg.payload === 'tare') weight = 0;\nweight = Math.max(0, weight);\nflow.set('w1', weight);\nmsg.payload = { scale_id: 'scale-01', weight: parseFloat(weight.toFixed(4)) };\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 300,
        "y": 140,
        "wires": [
            [
                "disp_s1",
                "mqtt_out"
            ]
        ]
    },
