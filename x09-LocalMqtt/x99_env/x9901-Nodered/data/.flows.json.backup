    {
        "id": "refresh_logic",
        "type": "function",
        "z": "scale_dashboard_tab",
        "name": "Format Periodic MQTT",
        "func": "const w1 = flow.get('w1') || 0;\nconst w2 = flow.get('w2') || 0;\nconst w3 = flow.get('w3') || 0;\n\nreturn [\n    { payload: { scale_id: 'scale-01', weight: parseFloat(w1.toFixed(4)) } },\n    { payload: { scale_id: 'scale-02', weight: parseFloat(w2.toFixed(3)) } },\n    { payload: { scale_id: 'scale-03', weight: parseFloat(w3.toFixed(2)) } }\n];",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 520,
        "y": 560,
        "wires": [
